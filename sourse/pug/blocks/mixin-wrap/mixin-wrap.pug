mixin authorization(data)
	+b.authorization&attributes(attributes)
		if data.empty
		else
			+e.left-side
				+e.logo(href='/'): +img('img/svg/logo.svg')
				+e.img-wrap
					.img-wrap-center(data-json= `json/${data.json}`)
				+e.footer
					h5!= data.title
					p!= data.text
		+e.right-side
			+e.logo.d-md-none(href='/'): +img('img/svg/logo.svg')
			+e.back-btn(href='/')
				+icon('chevron-left')
				| –ù–∞ –≥–ª–∞–≤–Ω—É—é
			block

mixin changePage()
	+b.btn-change-page
		mixin changePageBtn(span, text)
			+e.item(href='#')&attributes(attributes)
				+e.content
					span!= span
					+e.text!= text
				+icon('chevron-right')
		block

mixin sideScrollTop()
	+b.scrolToTop
		+e.wrap
			+icon('chevron-up')
			| –ù–∞–≤–µ—Ä—Ö

mixin contactItem(data)
	+b.contactItem(style=`--bgColor:${data.bgc};`)
		+e.img-wrap
			+img(`img/svg/contact-${data.img}.svg`)
		if data.descr
			+e.descr!= data.descr
		if data.link
			+e.link(href=`${data.adress}` target="_blank")!= data.link
		if data.time
			+e.schedule!= data.time
		if data.btn
			+e.btn.btn.btn-outline-secondary(href=`${data.adress}` target="_blank")!= data.btn

mixin navBlock()
	+b.nav-block
		+e.content.sticky-js 
			block

mixin wikiItem(data)
	+b.wikiItem(href="#")
		//- +img(`img/wikiItem-${data.img}.jpg`)
		+e.img-wrap.skeleton
			+img(`img/wikiItem-${data.img}.jpg`, 'bg')
		+e.content
			+e.head
				+e.step!= data.step
				if data.sub
					+e.subtitle!= data.sub
				if data.badge
					.badge.bg-success!= data.badge
			+e.title!= data.title
			if data.text
				p!= data.text

